{% extends 'layout.nj' %}

{% block content %}


<h1>Hello NJ</h1>

<form id="vote-form">
    <div id="target">
        {% for subject in subjects %}
        <input type="radio" id={{ subject.name }} name="vote" value={{ subject.name }}>
        <label for={{ subject.name }}>{{ subject.name }} have {{ subject.voteNum }} vote</label>
        {% endfor %}
    </div>
    <input type="submit" value="submit now">
    <div class="log"></div>
</form>
{% endblock %}